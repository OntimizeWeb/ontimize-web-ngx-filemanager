{
  "name": "ontimize-web-ngx-filemanager",
  "version": "3.0.0-rc.0",
  "description": "An implementation of file management system for Ontimize Web",
  "scripts": {
    "preinstall": "npm config set package-lock false",
    "rimraf": "rimraf",
    "webpack": "webpack",
    "test": "karma start",
    "clean": "rimraf dist tmp",
    "init-common": "npm run clean && gulp inline-templates",
    "end-common": "gulp copy-files && gulp filemanager.styles",
    "webpack:dev": "npm run init-common && webpack --config config/webpack.dev.js --progress && npm run end-common",
    "webpack:prod": "npm run init-common && webpack --config config/webpack.prod.js --progress && npm run end-common",
    "aot:compile": "node_modules/.bin/ngc -p tsconfig.aot.json",
    "aot": "npm run init-common && npm run aot:compile && npm run end-common",
    "pack": "npm run aot && cd dist && npm pack && cd .."
  },
  "keywords": [
    "angular",
    "ontimize-web-ngx",
    "ontimize-web-ngx-filemanager",
    "filemanager"
  ],
  "author": "IMATIA S.L.",
  "repository": {
    "type": "git",
    "url": "git@github.com:OntimizeWebf/ontimize-web-ngx-filemanager.git"
  },
  "license": "Apache-2.0",
  "main": "./index.js",
  "module": "./index.js",
  "typings": "./index.d.ts",
  "peerDependencies": {
    "typescript": "2.4.2"
  },
  "devDependencies": {
    "@angular/animations": "5.2.9",
    "@angular/cdk": "5.2.4",
